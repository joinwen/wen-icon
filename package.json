{
  "name": "wen-icon",
  "version": "1.0.4",
  "description": "making svg into vue component integrated with figma",
  "main": "dist/main.js",
  "module": "src/main.js",
  "scripts": {
    "generate": "node src/tools/collect-components.js",
    "dev": "npm run generate && webpack serve --open --env file=env.development",
    "build:preview": "npm run generate && webpack --env file=env.production",
    "build": "npm run generate && webpack --config webpack.config.prod.js --env file=env.production",
    "gh-pages": "gh-pages -d preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joinwen/wen-icon.git"
  },
  "keywords": [
    "svg",
    "vue",
    "component",
    "figma"
  ],
  "author": "cywen",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "babel-loader": "^8.2.2",
    "cheerio": "^1.0.0-rc.9",
    "css-loader": "^5.2.4",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.9.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.2",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "postcss-loader": "^4.2.0",
    "svgo": "^2.3.0",
    "vue-loader": "^15.9.7",
    "vue-style-loader": "^4.1.3",
    "vue-svg-icon-loader": "^2.1.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@babel/runtime-corejs3": "^7.14.0",
    "vue": "^2.6.12",
    "vue-svg-component-runtime": "^1.0.1"
  }
}
